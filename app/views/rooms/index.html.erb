<h1>ルーム一覧</h1>
<table>
  <thead>
    <tr>
      <th>ゲーム名</h2></th>
    </tr>
  </thead>
  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= link_to room.name, room_path(room.id)%></td>
        <td><%= button_to 'delete', room, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to '追加', new_room_path(@game_id) %>